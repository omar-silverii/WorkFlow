{
  "StartNodeId": "nStart",
  "Nodes": {
    "nStart": {
      "Id": "nStart",
      "Type": "util.start",
      "Parameters": {}
    },
    "nDoc": {
      "Id": "nDoc",
      "Type": "doc.entrada",
      "Parameters": {
        "salida": "documento"
      }
    },
    "nIf": {
      "Id": "nIf",
      "Type": "control.if",
      "Parameters": {
        "expression": "${documento.tieneFirma} == true"
      }
    },
    "nEmail": {
      "Id": "nEmail",
      "Type": "email.send",
      "Parameters": {
        "to": [
          "omar.silverii@edi-sa.com"
        ],
        "subject": "Seguro sin firma",
        "html": "<b>Falta firma</b>"
      }
    },
    "nEnd": {
      "Id": "nEnd",
      "Type": "util.end",
      "Parameters": {}
    }
  },
  "Edges": [
    {
      "From": "nStart",
      "To": "nDoc",
      "Condition": "always"
    },
    {
      "From": "nDoc",
      "To": "nIf",
      "Condition": "always"
    },
    {
      "From": "nIf",
      "To": "nEnd",
      "Condition": "true"
    },
    {
      "From": "nIf",
      "To": "nEmail",
      "Condition": "false"
    },
    {
      "From": "nEmail",
      "To": "nEnd",
      "Condition": "always"
    }
  ]
}